generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AirQuality {
  id        String   @id @default(uuid())
  city      String
  state     String
  country   String
  location  Json
  pollution Json
  weather   Json
  createdAt DateTime @default(now()) @map("created_at")

  @@index([city])
  @@map("air_quality")
} 